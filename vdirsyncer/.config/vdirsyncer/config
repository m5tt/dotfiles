[general]
status_path = "~/.config/vdirsyncer/status/"

[pair calendar]
a = "calendar_local"
b = "calendar_remote"
collections = ["from a", "from b"]

[storage calendar_local]
type = "filesystem"
path = "~/.local/share/calendar/"
fileext = ".ics"

[storage calendar_remote]
type = "caldav"
url = "http://192.168.1.136:5232/caldav/Calendar.ics/"
